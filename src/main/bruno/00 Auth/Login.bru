meta {
  name: Login
  type: http
  seq: 2
}

post {
  url: {{server}}/api/auth/login
  body: json
  auth: none
}

body:json {
  {
    "login": "user",
    "password": "user"
  }
}

assert {
  res.status: eq 200
}

script:post-response {
  bru.setVar("access_token", res.body.token)
  bru.setVar("player_id", res.body.id)
}
