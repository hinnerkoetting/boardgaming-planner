meta {
  name: Register
  type: http
  seq: 1
}

post {
  url: {{server}}/api/auth/register
  body: json
  auth: none
}

body:json {
  {
    "login": "user",
    "password": "user"
  }
}

script:post-response {
  if (res.body.token) {
    bru.setVar("access_token", res.body.token)
  
    bru.setVar('playerId',res.body.id)
  }
}
