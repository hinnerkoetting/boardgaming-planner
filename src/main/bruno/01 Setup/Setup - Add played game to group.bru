meta {
  name: Setup - Add played game to group
  type: http
  seq: 6
}

post {
  url: http://localhost:8080/api/gameGroups/{{gameGroupId}}/games
  body: json
  auth: none
}

headers {
  Authorization: Bearer {{access_token}}
}

body:json {
  {
    "id": {{gameId}}
  }
}

assert {
  res.status: eq 201
}
