meta {
  name: Setup - Create gamegroup
  type: http
  seq: 5
}

post {
  url: {{server}}/api/gameGroups
  body: json
  auth: none
}

headers {
  Authorization: Bearer {{access_token}}
}

body:json {
  {
    "name": "Boardgames"
  }
}

assert {
  res.status: eq 201
}

script:post-response {
  if (res.status === 201) {
    bru.setVar("gameGroupId", res.body.id)
  }
}
