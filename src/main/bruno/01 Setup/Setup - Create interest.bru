meta {
  name: Setup - Create rating
  type: http
  seq: 8
}

put {
  url: http://localhost:8080/api/ratings
  body: json
  auth: none
}

headers {
  Authorization: Bearer {{access_token}}
}

body:json {
  {
    "playerId": {{playerId}},
    "gameId": {{gameId}},
    "gameGroupId": {{gameGroupId}},
    "rating": 1
  }
}

assert {
  res.status: eq 201
}
