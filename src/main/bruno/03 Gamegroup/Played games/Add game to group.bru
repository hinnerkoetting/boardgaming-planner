meta {
  name: Add game to group
  type: http
  seq: 1
}

post {
  url: {{server}}/api/gameGroups/{{gameGroupId}}/games
  body: json
  auth: none
}

headers {
  Authorization: Bearer {{access_token}}
}

body:json {
  {
    "id": {{gameId}}
  }
}

vars:pre-request {
  gameId: 2
}

assert {
  res.status: eq 201
}
