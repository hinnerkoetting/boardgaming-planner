meta {
  name: Add player tag
  type: http
  seq: 3
}

post {
  url: {{server}}/api/gameGroups/{{gameGroupId}}/{{gameId}}/{{playerId}}/playerTag
  body: json
  auth: none
}

headers {
  Authorization: Bearer {{access_token}}
}

body:json {
  {
    "id": "{{tagId}}"
  }
}

vars:pre-request {
  gameGroupId: 1
  gameId: 1
  tagId: 3
}

assert {
  res.status: eq 200
}
