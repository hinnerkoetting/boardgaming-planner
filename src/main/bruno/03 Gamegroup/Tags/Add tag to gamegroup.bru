meta {
  name: Add tag to gamegroup
  type: http
  seq: 1
}

post {
  url: {{server}}/api/gameGroups/{{gameGroupId}}/{{gameId}}/gameGroupTag
  body: json
  auth: none
}

headers {
  Authorization: Bearer {{access_token}}
}

body:json {
  {
    "id": "{{tagId}}"
  }
}

vars:pre-request {
  role: ROLE_ADMIN
}

assert {
  res.status: eq 200
}
