meta {
  name: Remove tag from gamegroup
  type: http
  seq: 2
}

delete {
  url: {{server}}/api/gameGroups/{{gameGroupId}}/{{gameId}}/gameGroupTag/{{tagId}}
  body: none
  auth: none
}

headers {
  Authorization: Bearer {{access_token}}
}

vars:pre-request {
  role: ROLE_ADMIN
}

assert {
  res.status: eq 200
}
