meta {
  name: Add tag to game
  type: http
  seq: 1
}

post {
  url: http://localhost:8080/api/games/{{gameId}}/globalTag
  body: json
  auth: none
}

headers {
  Authorization: Bearer {{access_token}}
}

body:json {
  {
    "id": "{{tagId}}"
  }
}

vars:pre-request {
  role: ROLE_ADMIN
}

assert {
  res.status: eq 200
}
