meta {
  name: Search bgg
  type: http
  seq: 1
}

get {
  url: http://localhost:8080/api/bgg/search/{{searchTerm}}
  body: none
  auth: none
}

headers {
  Authorization: Bearer {{access_token}}
}

vars:pre-request {
  searchTerm: Frosthaven
}

assert {
  res.status: eq 200
}

script:post-response {
  const bggIds = res.body.map(game => game.id).join(',')
  bru.setVar("bggIds", bggIds)
}
