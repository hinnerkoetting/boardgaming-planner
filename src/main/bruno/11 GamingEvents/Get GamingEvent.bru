meta {
  name: Get GamingEvent
  type: http
  seq: 4
}

get {
  url: {{server}}/api/gamingEvents/{{gamingEventId}}
  body: none
  auth: inherit
}

headers {
  Authorization: Bearer {{access_token}}
}

vars:pre-request {
  gamingEventId: 2
}

script:post-response {
  const body = res.body;
  if (body.length > 0) {
    bru.setVar("gamingEventId", body[0].id)
  }
}
