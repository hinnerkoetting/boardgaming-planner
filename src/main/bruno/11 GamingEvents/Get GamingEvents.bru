meta {
  name: Get GamingEvents
  type: http
  seq: 1
}

get {
  url: {{server}}/api/gameGroup/{{gameGroupId}}/gamingEvents?onlyNext=false&number=5
  body: none
  auth: inherit
}

params:query {
  onlyNext: false
  number: 5
}

headers {
  Authorization: Bearer {{access_token}}
}

vars:pre-request {
  gameGroupId: 1
}

script:post-response {
  const body = res.body;
  if (body.length > 0) {
    bru.setVar("gamingEventId", body[0].id)
  }
}
