meta {
  name: Add participant
  type: http
  seq: 1
}

post {
  url: {{server}}/api/gamingEvents/{{gamingEventId}}/player
  body: json
  auth: none
}

headers {
  Authorization: Bearer {{access_token}}
}

body:json {
  {
    "playerId": {{playerId}},
    "participationStatus": "MAYBE",
    "comment": "Looking forward"
  }
}

vars:pre-request {
  gameGroupId: 1
}

assert {
  res.status: eq 200
}
